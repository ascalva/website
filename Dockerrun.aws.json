{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "054942746160.dkr.ecr.us-west-2.amazonaws.com/personal-website:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "5000"
    }
  ],
  "environment": [
      {
          "name:": "SECRET_KEY",
          "value:": "${SECRET_KEY}"
      },
      {
          "name:": "SECURITY_PASSWORD_SALT",
          "value:": "${SECURITY_PASSWORD_SALT}"
      },
      {
          "name:": "WORKER_NUM",
          "value:": "${WORKER_NUM}"
      }
  ]
}
